{
  "name": "ui-regression-tests",
  "version": "1.0.0",
  "description": "This is UI part of CBA Tests",
  "main": "index.js",
  "scripts": {
    "cypress:open": "node_modules/.bin/cypress open",
    "cypress:chrome": "node_modules/.bin/cypress run --browser chrome --spec 'cypress/integration/*'",
    "cypress:firefox": "node_modules/.bin/cypress run --browser firefox --spec 'cypress/integration/*'",
    "cypress:all": "npm-run-all -sc cypress:chrome cypress:firefox"
  },
  "author": "Amjad Ali",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^4.2.0",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "npm-run-all": "^4.1.5"
  }
}
